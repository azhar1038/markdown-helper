{
  "name": "markdown-mate",
  "displayName": "Markdown Mate",
  "description": "Some commands to help with Markdown files",
  "version": "0.0.1",
  "icon": "images/logo.png",
  "preview": true,
  "author": {
    "name": "Md Azharuddin",
    "email": "mdazhar.1038@gmail.com",
    "url": "https://azhar1038.github.io/"
  },
  "publisher": "Azhar",
  "repository": {
    "type": "git",
    "url": "https://github.com/azhar1038/markdown-mate"
  },
  "homepage": "https://github.com/azhar1038/markdown-mate",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "markdown",
    "table of contents",
    "toc",
    "tool",
    "helper",
    "shortcut"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-mate.insertToc",
        "title": "Markdown: Insert Table of Contents"
      },
      {
        "command": "markdown-mate.removeToc",
        "title": "Markdown: Remove Table of Contents"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
